{
  "name": "@endemolshinegroup/github-backup-cli",
  "description": "A CLI tool to back up GitHub repositories",
  "version": "0.0.1",
  "license": "MIT",
  "bin": {
    "github-backup-cli": "dist/index.js"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": {
    "name": "Endemol Shine Group Technology",
    "url": "https://github.com/EndemolShineGroup"
  },
  "homepage": "https://github.com/EndemolShineGroup/github-backup-cli",
  "repository": {
    "type": "git",
    "url": "https://github.com/EndemolShineGroup/github-backup-cli.git"
  },
  "engines": {
    "node": ">=8.0.0",
    "yarn": ">=1.3.0"
  },
  "scripts": {
    "commit": "git-cz",
    "build": "rimraf dist/ && tsc",
    "build:docs": "rimraf docs/api && typedoc --out docs/api --target es6 --theme minimal --mode file src",
    "build:watch": "tsc --watch",
    "lint": "tslint -p tsconfig.json -t codeFrame 'src/**/*.ts' -e 'src/**/*.spec.ts'",
    "pretest": "rimraf coverage/",
    "test": "jest --no-cache",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@octokit/rest": "^15.13.0",
    "aws-sdk": "^2.329.0",
    "cli-progress": "^2.1.0",
    "commander": "^2.18.0",
    "debug": "^4.0.1",
    "isomorphic-fetch": "^2.2.1",
    "promisify-child-process": "^2.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^7",
    "@commitlint/config-conventional": "^7",
    "@commitlint/prompt": "^7",
    "@semantic-release/changelog": "^3",
    "@semantic-release/git": "^7",
    "@types/cli-progress": "^1.8.0",
    "@types/debug": "^0.0.31",
    "@types/isomorphic-fetch": "^0.0.34",
    "@types/jest": "^23",
    "@types/node": "^10",
    "commitizen": "^3.0.2",
    "husky": "^1.1.1",
    "jest": "^23",
    "lint-staged": "^7",
    "prettier": "^1",
    "rimraf": "^2",
    "semantic-release": "^15",
    "ts-jest": "^23.10.0-beta.1",
    "tslint": "^5",
    "tslint-config-esg": "github:EndemolShineGroup/tslint-config-esg",
    "tslint-config-prettier": "^1",
    "typedoc": "^0.12.0",
    "typescript": "^3"
  },
  "publishConfig": {
    "access": "public"
  }
}
